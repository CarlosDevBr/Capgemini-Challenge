

public static class validadorSenhas {

	
	public static boolean containNumbem(String senha) {
		String nums = "1234567890";
		//CharSequence chars = nums.toCharArray();
		char[] chars = nums.toCharArray();
		
		
		for(char ch : chars) {
			if(senha.indexOf(ch) != -1) {
				return true;
			}
		}
		
		return false;		
	}
}
